{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "CONFLUENCE_API_URL": "@confluence_api_url",
    "CONFLUENCE_API_TOKEN": "@confluence_api_token"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
